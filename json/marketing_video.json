{
  "description": "Sample marketing video analyzer",
  "scenario": "videoShot",
  "config": {
    "returnDetails": true
  },
  "fieldSchema": {
    "fields": {
      "Description": {
        "type": "string",
        "description": "Detailed summary of the video segment, focusing on product characteristics, lighting, and color palette."
      },
      "Sentiment": {
        "type": "string",
        "method": "classify",
        "enum": [
          "Positive",
          "Neutral",
          "Negative"
        ]
      }
    }
  }
}